<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/bb_level13"
    android:background="@drawable/analysis_background"
    tools:context="com.wireout.yantra.com.wireout.GameBrainBooster.Levels.Level13Activity">


    <com.wireout.GameBrainBooster.CustomView
        android:id="@+id/custom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textColor="#000"
        android:clickable="true"
        android:text="I'm 3 years old?"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="200dp"
        android:layout_height="250dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/cake" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:visibility="invisible"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/brain_op_buttonanim"
        android:onClick="onSkip"
        android:text="skip >>"
        android:layout_marginBottom="20dp"/>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="25dp"
        android:layout_height="55dp"
        android:layout_marginTop="3dp"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8"
        android:visibility="invisible"
        android:layout_marginStart="69dp"
        app:srcCompat="@drawable/flame" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="25dp"
        android:layout_height="55dp"
        android:layout_marginTop="3dp"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_alignEnd="@+id/imageButton8"
        android:layout_marginEnd="55dp"
        android:visibility="invisible"
        app:srcCompat="@drawable/flame" />

</RelativeLayout>
